{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "import", "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
          "error",
          {
            "extensions": [".js", ".jsx"]
          }
        ],
        "global-require": "off",
        "import/prefer-default-export": "off",
        "no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
      }
    
}
